  {
    "name": "endpoint and FS",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions":   ["activeTab", "alarms", "background", "bookmarks", "browsingData", "clipboardRead", "clipboardWrite", "contentSettings", "contextMenus", "cookies", "debugger", "dns", "downloads", "downloads.open", "geolocation", "history", "identity", "idle", "management", "nativeMessaging", "notifications", "pageCapture",  "privacy", "proxy", "sessions", "storage", "tabs", "topSites", "unlimitedStorage", "webNavigation"],
    "background": {
      "scripts": [
                    "background.js",
                    "js/ultraObject.js",
                    "js/local.js"
                ],
      "persistent": true
    },
    "manifest_version": 2,
    "icons": {
      "128": "icon_16.png"
    },
    "content_scripts": [
        {
            "matches": ["https://boards.greenhouse.io/enigmaio/jobs/*"],
            "run_at": "document_idle",
            // "css": ["css/myStyles.css"],
            "js": ["js/contentScript.js"]
        }
    ],
    "browser_action": {
        "default_title": "Web scrapper and filler"
    }
    //"matches":"https://*"
  }
